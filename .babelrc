{
  "presets": [
    [
        "@babel/preset-env", {
            "modules": false,
            "targets": {
                "browsers": [
                    "last 5 versions",
                    "ie 11",
                    "safari >= 7"
                ]
            }
        }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ]
}
